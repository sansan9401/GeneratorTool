+def remove_diag(diag, model=None):
+    llg_count=0
+    for vertex in diag['vertices']:
+        lepton_count=0
+        dy_count=0
+        for leg in vertex['legs']:
+            if abs(leg['id']) in [11,13,15] : lepton_count+=1
+            if leg['id'] in [22,23] : dy_count+=1
+        if lepton_count==2 and dy_count==1: llg_count+=1
+    if llg_count!=2: return True
+    else: return False
